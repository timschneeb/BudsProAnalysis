<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_gravity="center_horizontal" android:orientation="vertical" android:id="@+id/sesl_spinning_date_time_picker_layout" android:layout_width="match_parent" android:layout_height="wrap_content">
    <com.android.internal.widget.DialogViewAnimator android:gravity="center" android:id="@+id/sesl_spinning_date_picker_view_animator" android:layout_width="match_parent" android:layout_height="wrap_content" android:measureAllChildren="true" android:inAnimation="@anim/sesl_date_picker_fade_in" android:outAnimation="@anim/sesl_date_picker_fade_out">
        <LinearLayout android:id="@+id/sesl_spinning_date_picker_view" android:layout_width="match_parent" android:layout_height="@dimen/sesl_spinning_date_picker_height">
            <View android:id="@+id/sesl_spinning_date_time_picker_margin_view_left" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="@integer/sesl_spinning_date_time_picker_margin_view_left_weight"/>
            <View android:id="@+id/sesl_spinning_date_time_picker_empty_view_left" android:visibility="gone" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="@integer/sesl_spinning_date_time_picker_empty_view_left_weight"/>
            <androidx.picker.widget.SeslSpinningDatePickerSpinner android:gravity="center" android:id="@+id/sesl_spinning_date_picker_spinner_view" android:focusable="true" android:visibility="visible" android:layout_width="0dp" android:layout_height="@dimen/sesl_date_picker_spinner_height" android:layout_marginTop="@dimen/sesl_spinning_date_picker_margin_top" android:layout_weight="@integer/sesl_spinning_date_picker_spinner_view_weight"/>
            <View android:id="@+id/sesl_spinning_date_picker_margin_view_center" android:visibility="gone" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="4"/>
        </LinearLayout>
    </com.android.internal.widget.DialogViewAnimator>
</LinearLayout>
